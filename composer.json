{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.3",
        "filament/spatie-laravel-media-library-plugin": "^v3.3",
        "filament/spatie-laravel-settings-plugin": "^v3.3",
        "filament/spatie-laravel-tags-plugin": "^v3.3",
        "filament/spatie-laravel-translatable-plugin": "^v3.3",
        "flowframe/laravel-trend": "^v0.4",
        "guzzlehttp/guzzle": "^v7.9",
        "laravel/framework": "^12.14",
        "laravel/horizon": "^v5.32",
        "laravel/sanctum": "^v4.1",
        "laravel/tinker": "^v2.10",
        "squirephp/countries-en": "^v3.9",
        "squirephp/currencies-en": "^v3.9"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^v3.15",
        "fakerphp/faker": "^v1.24",
        "filament/upgrade": "^4.0",
        "larastan/larastan": "^v3.4",
        "laravel/pint": "^v1.22",
        "laravel/sail": "^v1.43",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^v8.8",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpunit/phpunit": "^12.1",
        "spatie/laravel-ignition": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "cs": [
            "pint"
        ],
        "pint": "pint",
        "test:phpstan": "phpstan analyse",
        "test": [
            "@test:phpstan"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
